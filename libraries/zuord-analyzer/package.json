{
  "name": "zuord-analyzer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "typescriptServerPlugins": [
      {
        "name": "zuord-plugin",
        "enableForWorkspaceType": [
          "all"
        ]
      }
    ],
    "commands": [
      {
        "command": "zuord-analyzer.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "script:zuord-plugin": "node scripts/zuord-plugin.js",
    "postinstall": "npm run script:zuord-plugin"
  }
}
